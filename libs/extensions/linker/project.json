{
  "name": "linker",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/extensions/linker/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/extensions/linker",
        "main": "libs/extensions/linker/src/index.ts",
        "tsConfig": "libs/extensions/linker/tsconfig.lib.json",
        "assets": [
          "libs/extensions/linker/*.md"
        ]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/libs/extensions/{projectName}"
      }
    }
  },
  "tags": [],
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/libs/extensions/{projectName}",
        "currentVersionResolver": "git-tag"
      }
    }
  }
}