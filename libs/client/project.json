{
  "name": "client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/{projectName}/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/{projectName}",
        "main": "libs/{projectName}/src/index.ts",
        "tsConfig": "libs/{projectName}/tsconfig.lib.json",
        "assets": [
          "libs/{projectName}/*.md"
        ]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/libs/{projectName}"
      }
    }
  },
  "tags": [],
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/libs/{projectName}",
        "currentVersionResolver": "git-tag"
      }
    }
  }
}